{
  "name": "learn-extension",
  "displayName": "learn-extension",
  "description": "Learn about lines of code while you edit",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:rust"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "learn",
          "title": "Learn",
          "icon": "./resources/brain.svg"
        }
      ]
    },
    "views": {
      "learn": [
        {
          "id": "concepts",
          "name": "Concepts"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "tslint": "^5.20.0",
    "typescript": "^3.7",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "@types/markdown-it": "^0.0.9",
    "lodash": "^4.17.21",
    "markdown-it": "^10.0.0",
    "vscode-languageclient": "^5.2.1",
    "web-tree-sitter": "^0.15.11"
  }
}
